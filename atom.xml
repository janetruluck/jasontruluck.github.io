---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Jason Truluck's Dev Blog</title>
  <link href="http://jasontruluck.github.io/feed/" rel="self"/>
  <link href="http://jasontruluck.github.io"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://jasontruluck.github.io</id>
  <author>
    <name>Jason Truluck</name>
    <email>jason.truluck@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://jasontruluck.github.io{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://jasontruluck.github.io{{ post.id }}</id>
    <content type="html">
      {{ post.content | xml_escape }}
    </content>
  </entry>
  {% endfor %}
</feed>
